{
    "name": "upstate-php/hackgreenville",
    "description": "Website for the HackGreenville",
    "keywords": [
        "updatePHP",
        "hack",
        "Greenville"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "filament/filament": "^3.3",
        "firebase/php-jwt": "^6.10",
        "glhd/aire-bootstrap": "^2.1",
        "glhd/conveyor-belt": "*",
        "guzzlehttp/guzzle": "^7.0.1",
        "hack-greenville/api": "*",
        "hack-greenville/event-importer": "*",
        "hack-greenville/slack-events-bot": "*",
        "internachi/modular": "^2.0",
        "knuckleswtf/scribe": "^5.2",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.2",
        "laravel/slack-notification-channel": "^2.4",
        "laravel/tinker": "^2.0",
        "malzariey/filament-daterangepicker-filter": "^2.7",
        "scyllaly/hcaptcha": "^4.4",
        "spatie/icalendar-generator": "^2.9",
        "spatie/laravel-data": "*"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "ext-sqlite3": "*",
        "barryvdh/laravel-debugbar": "^3.8",
        "barryvdh/laravel-ide-helper": "*",
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.10",
        "laravel/sail": "^1.5",
        "laravel/telescope": "*",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^7.7",
        "phpunit/phpunit": "^10.2",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "scripts": {
        "lint": [
            "./vendor/bin/pint --quiet"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan ide-helper:generate --ansi --no-interaction || echo 'IDE helper not available'",
            "@php artisan ide-helper:meta --ansi --no-interaction || echo 'IDE helper not available'",
            "@php artisan ide-helper:models -N --no-interaction || echo 'IDE helper not available'"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall"
        ],
        "serve": [
            "php artisan serve"
        ],
        "logs:tail": [
            "tail -f storage/logs/laravel.log"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"composer serve\" \"php artisan queue:listen --tries=1\" \"composer logs:tail\" \"yarn dev --host\" --names=server,queue,logs,vite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {}
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "path",
            "url": "app-modules/*",
            "options": {
                "symlink": true
            }
        }
    ]
}
